write2(all(isnan(write2)'),:) = [];

red1(isnan(red1)) = [];
x = x(find( ~ isnan(x)))
red1 = red1( ~ isnan(red1));

red1(all(red1==0,2),:) = [];
i = find(isnan(red1))

Var2(find(Var2 == 0)) = []

red1use = datapre2(red1);

red1ord = datapre3(red1use);


